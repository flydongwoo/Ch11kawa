백준 1번째 문제
----------------
#include <stdio.h>
#include <string.h>

int isGMS(char num[8]);

int main(void)
{
	int target = 0;
	scanf("%d", &target);
	char string[8];
	sprintf(string, "%d", target);

	while (isGMS(string)) {
		target--;
		sprintf(string, "%d", target);
	}
	printf("%d", target);
}


int isGMS(char num[8])
{
	for (int i = 0; i < strlen(num); i++) {
		if (num[i] != '4' && num[i] != '7') {
			return 1;
		}
	}
	return 0;
}

----------------
백준 2번째 문제
----------------

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void transNum(char a, char b, int* A, int* B);
char transDNA(int num1, int num2);
int main(void)
{
    int length = 0;
    char* series;
    scanf("%d", &length);
    series = (char*)malloc(sizeof(char) * (length + 1));
    scanf("%s", series);
    for (int i = strlen(series)-1; i > 0; i--) {
        int A = 0, B = 0;
        transNum(series[i], series[i - 1], &A, &B);
        series[i - 1] = transDNA(A, B);
    }
    printf("%c\n", series[0]);
    free(series);
    return 0;
}
void transNum(char a, char b, int* A, int* B)
{
    if (a == 'A') { *A = 1; }      
    else if (a == 'G') { *A = 3; }
    else if (a == 'T') { *A = 5; }
    else { *A = 11; }

    if (b == 'A') { *B = 1; }
    else if (b == 'G') { *B = 3; }
    else if (b == 'T') { *B = 5; }
    else { *B = 11; }
}

char transDNA(int num1, int num2)
{
    int result = num1 + num2;
    if (result == 12 || result == 8 || result == 2) { return 'A'; }
    else if (result == 6 || result == 16) { return 'G'; }
    else if (result == 4 || result == 22) { return 'C'; }
    else { return 'T'; }
}
