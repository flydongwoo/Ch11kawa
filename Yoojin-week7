// programmers peob2

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#define LEN_INPUT1 11
#define LEN_INPUT2 11

int main(void) {
	char s1[LEN_INPUT1];
	char s2[LEN_INPUT2];
	scanf("%s %s", s1, s2);

	printf("%s%s", s1, s2);
	return 0;
}

