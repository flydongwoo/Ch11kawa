// programmers peob2

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#define LEN_INPUT1 11
#define LEN_INPUT2 11

int main(void) {
	char s1[LEN_INPUT1];
	char s2[LEN_INPUT2];
	scanf("%s %s", s1, s2);

	printf("%s%s", s1, s2);
	return 0;
}

//-----------------------------------------------
// programmers prob3

#define _CRT_SECURE_NO_WARNINGS
#define LEN_INPUT 11
#include <stdio.h>
#include <string.h>

void printAry(char* str, int size);

int main(void) {
	char s1[LEN_INPUT];
	scanf("%s", s1);
	int size = strlen(s1);

	printAry(s1, size);

	return 0;
}

void printAry(char* str, int size) {
	for (int i = 0; i < size; i++)
		printf("%c\n", str[i]);
}

//----------------------------------------------
// programmers prob4

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void) {
	int a;
	scanf("%d", &a);

	if (a % 2 == 0)
		printf("%d is even\n", a);
	else printf("%d is odd\n", a);
	return 0;
}

