// programmers prob4

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void) {
	int a;
	scanf("%d", &a);

	if (a % 2 == 0)
		printf("%d is even\n", a);
	else printf("%d is odd\n", a);
	return 0;
}

